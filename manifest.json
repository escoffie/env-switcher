{
  "manifest_version": 3,
  "name": "URL Env Switcher",
  "description": "Modify and manage URLs",
  "version": "1.0",
  "permissions": ["storage", "tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon128.ico"
  },
  "icons": {
    "128": "icons/icon128.ico"
  }
}
